<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.klaushackner.alcoholtest"
    android:versionName="0.3">

    <uses-sdk android:minSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="de.klaushackner.alcoholtest.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="de.klaushackner.alcoholtest.CreateUser"></activity>
        <activity android:name="de.klaushackner.alcoholtest.EditUser"></activity>
    </application>

</manifest>